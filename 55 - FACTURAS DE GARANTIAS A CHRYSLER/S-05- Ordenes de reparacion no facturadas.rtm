      object daSQL1: TdaSQL
        GuidCollationType = gcMSSQLServer
        DatabaseName = 'SrvConnection'
        DatabaseType = dtMSSQLServer
        DataPipelineName = 'Ventas'
        LinkColor = clMaroon
        LinkType = ltMagicSQL
        MaxSQLFieldAliasLength = 25
        SQLText.Strings = (
          'SELECT SAFACT.CodClie, SAFACT.CodOper, '
          '       SAFACT.CodVend, SAFACT.Descrip, '
          '       SAFACT.FechaE, SAFACT.FechaV, SAFACT.ID3, '
          '       SAFACT.Monto, SAFACT.MtoTax, '
          '       SAFACT.NumeroD, SAFACT.Signo, '
          '       SAFACT.TExento, SAFACT.TGravable, '
          '       SAFACT.TipoFac, SAFACT.TotalPrd, '
          '       SAFACT.TotalSrv, SAFACT_01.Apertura_OR, '
          '       SAFACT_01.Cierre_OR, '
          '       SAFACT_01.Kilometraje, '
          '       SAFACT_01.Liquidacion, SAFACT_01.Placa, '
          '       SAFACT_01.Status, SAFACT_02.Modelo'
          'FROM SAFACT SAFACT'
          '      INNER JOIN SAFACT_01 SAFACT_01 ON '
          '     (SAFACT_01.NumeroD = SAFACT.NumeroD)'
          '      INNER JOIN SAFACT_02 SAFACT_02 ON '
          '     (SAFACT_02.NumeroD = SAFACT_01.NumeroD)'
          'WHERE ( SAFACT.TipoFac IN ('#39'G'#39','#39'Z'#39') )'
          '       AND ( SAFACT.CodOper = '#39'01-301'#39' )'
          
            '       AND ( SAFACT_01.Apertura_OR BETWEEN CONVERT(DATETIME,'#39'200' +
            '8-01-01'#39',120) AND CONVERT(DATETIME,'#39'2008-07-31'#39',120) )'
          
            '       AND ( SAFACT_01.Cierre_OR BETWEEN CONVERT(DATETIME,'#39'2008-' +
            '01-01'#39',120) AND CONVERT(DATETIME,'#39'2008-07-31'#39',120) )'
          'ORDER BY SAFACT.NumeroD')
