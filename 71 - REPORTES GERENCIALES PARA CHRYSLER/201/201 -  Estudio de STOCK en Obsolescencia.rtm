      object daSQL1: TdaSQL
        GuidCollationType = gcMSSQLServer
        DatabaseName = 'SrvConnection'
        DatabaseType = dtMSSQLServer
        DataPipelineName = 'Inventario'
        LinkColor = clMaroon
        LinkType = ltMagicSQL
        MaxSQLFieldAliasLength = 25
        SQLText.Strings = (
          'SELECT SAPROD.Activo, SAPROD.CantEmpaq, '
          '       SAPROD.CodInst, SAPROD.CodProd, '
          '       SAPROD.Compro, SAPROD.CostAct, '
          '       SAPROD.CostAnt, SAPROD.CostPro, '
          '       SAPROD.DEsComi, SAPROD.DEsComp, '
          '       SAPROD.Descrip, SAPROD.Descrip2, '
          '       SAPROD.Descrip3, SAPROD.DEsLote, '
          '       SAPROD.DEsSeri, SAPROD.DEsVence, '
          '       SAPROD.EsEmpaque, SAPROD.EsEnser, '
          '       SAPROD.EsExento, SAPROD.EsImport, '
          '       SAPROD.EsOferta, SAPROD.EsPesa, '
          '       SAPROD.ExDecimal, SAPROD.Existen, '
          '       SAPROD.ExistenCon, SAPROD.ExUnidad, '
          '       SAPROD.ExUnidadCon, SAPROD.FechaUC, '
          '       SAPROD.FechaUV, SAPROD.Marca, '
          '       SAPROD.Maximo, SAPROD.Minimo, '
          '       SAPROD.Pedido, SAPROD.Peso, '
          '       SAPROD.Precio1, SAPROD.Precio2, '
          '       SAPROD.Precio3, SAPROD.PrecioU, '
          '       SAPROD.PrecioU2, SAPROD.PrecioU3, '
          '       SAPROD.Refere, SAPROD.Tara, '
          '       SAPROD.UndEmpaq, SAPROD.UndVol, '
          '       SAPROD.Unidad, SAPROD.Volumen, '
          '       saprod.costpro*saprod.existen saprod_costpro_saprod_exi'
          'FROM SAPROD SAPROD'
          'WHERE ('#39'c'#39' <> '#39'c'#39' ) AND ('
          '      ( SAPROD.CodInst NOT IN (11,12) )'
          '       AND ( SAPROD.Existen > 0 ))')
