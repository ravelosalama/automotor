      object daSQL1: TdaSQL
        GuidCollationType = gcMSSQLServer
        DatabaseName = 'SrvConnection'
        DatabaseType = dtMSSQLServer
        DataPipelineName = 'Cuentas_pagar'
        LinkColor = clMaroon
        LinkType = ltMagicSQL
        MaxSQLFieldAliasLength = 25
        SQLText.Strings = (
          'SELECT SAACXP.CodOper, SAACXP.CodProv, '
          '       SAACXP.FechaE, SAACXP.FechaV, '
          '       SAACXP.NroUnico, SAACXP.NumeroD, '
          '       SAACXP.TipoCxP, SAPAGCXP.BaseReten, '
          '       SAPAGCXP.CodRete, SAPAGCXP.Descrip, '
          '       SAPAGCXP.EsReten EsReten_2, '
          '       SAPAGCXP.FechaE FechaE_2, SAPAGCXP.FechaO, '
          '       SAPAGCXP.Monto Monto_2, SAPAGCXP.NroPpal, '
          '       SAPAGCXP.NroRegi NroRegi_2, '
          '       SAPAGCXP.NroUnico NroUnico_2, '
          '       SAPAGCXP.NumeroD NumeroD_2, '
          '       SAPAGCXP.TipoCxP TipoCxP_2, '
          '       SAACXP_2.NroUnico NroUnico_3, '
          '       SAACXP_2.FechaE FechaE_3, '
          '       SAACXP_2.FechaV FechaV_2, '
          '       SAACXP_2.CodOper CodOper_2, '
          '       SAACXP_2.NumeroD NumeroD_3, '
          '       SAACXP_2.TipoCxP TipoCxP_3, '
          '       SAACXP_2.Document Document_2, '
          '       SAACXP_2.NroCtrol, SAACXP_2.BaseImpo'
          'FROM SAACXP SAACXP'
          '      LEFT OUTER JOIN SAPAGCXP SAPAGCXP ON '
          '     (SAPAGCXP.NroPpal = SAACXP.NroUnico)'
          '      LEFT OUTER JOIN SAACXP SAACXP_2 ON '
          '     (SAACXP_2.NroUnico = SAPAGCXP.NroRegi)'
          'WHERE ( SAACXP.TipoCxP = '#39'21'#39' )'
          
            '       AND ( SAACXP.FechaE BETWEEN CONVERT(DATETIME,'#39'2010-01-01'#39 +
            ',120) AND CONVERT(DATETIME,'#39'2010-12-31'#39',120) )'
          'ORDER BY SAPAGCXP.CodRete')
        SQLType = sqSQL2
