      object daSQL1: TdaSQL
        GuidCollationType = gcMSSQLServer
        DatabaseName = 'SrvConnection'
        DatabaseType = dtMSSQLServer
        DataPipelineName = 'Ventas'
        LinkColor = clMaroon
        MaxSQLFieldAliasLength = 25
        SQLText.Strings = (
          'SELECT SAFACT.Cambio, SAFACT.CancelA, '
          '       SAFACT.CancelC, SAFACT.CancelE, '
          '       SAFACT.CancelG, SAFACT.CancelI, '
          '       SAFACT.CancelT, SAFACT.Ciudad, '
          '       SAFACT.CodClie, SAFACT.CodEsta, '
          '       SAFACT.CodOper, SAFACT.CodSucu, '
          '       SAFACT.CodUbic, SAFACT.CodUsua, '
          '       SAFACT.CodVend, SAFACT.CostoPrd, '
          '       SAFACT.CostoSrv, SAFACT.Descrip, '
          '       SAFACT.Descto1, SAFACT.Descto2, '
          '       SAFACT.DesctoP, SAFACT.DetalChq, '
          '       SAFACT.Direc1, SAFACT.Direc2, '
          '       SAFACT.Direc3, SAFACT.EsConsig, '
          '       SAFACT.Estado, SAFACT.Factor, '
          '       SAFACT.FechaE, SAFACT.FechaV, '
          '       SAFACT.Fletes, SAFACT.ID3, SAFACT.Monto, '
          '       SAFACT.MontoMEx, SAFACT.MtoComiCob, '
          '       SAFACT.MtoComiCobD, SAFACT.MtoComiVta, '
          '       SAFACT.MtoComiVtaD, SAFACT.MtoExtra, '
          '       SAFACT.MtoFinanc, SAFACT.MtoInt1, '
          '       SAFACT.MtoInt2, SAFACT.MtoTax, '
          '       SAFACT.NGiros, SAFACT.NIT, SAFACT.NMeses, '
          '       SAFACT.Notas1, SAFACT.Notas10, '
          '       SAFACT.Notas2, SAFACT.Notas3, '
          '       SAFACT.Notas4, SAFACT.Notas5, '
          '       SAFACT.Notas6, SAFACT.Notas7, '
          '       SAFACT.Notas8, SAFACT.Notas9, '
          '       SAFACT.NumeroD, SAFACT.NumeroF, '
          '       SAFACT.NumeroK, SAFACT.NumeroP, '
          '       SAFACT.OrdenC, SAFACT.Pais, '
          '       SAFACT.PctAnual, SAFACT.PctManejo, '
          '       SAFACT.RetenIVA, SAFACT.Signo, '
          '       SAFACT.Status, SAFACT.Telef, '
          '       SAFACT.TExento, SAFACT.TGravable, '
          '       SAFACT.TipoFac, SAFACT.TotalPrd, '
          '       SAFACT.TotalSrv, SAFACT_01.Liquidacion, '
          '       SAFACT_01.Status Status_2, '
          '       SAFACT_01.Codigo, SAFACT_01.Placa'
          'FROM SAFACT SAFACT'
          '      INNER JOIN SAFACT_01 SAFACT_01 ON '
          '     (SAFACT_01.NumeroD = SAFACT.NumeroD)'
          '      AND (SAFACT_01.TipoFac = SAFACT.TipoFac)'
          'WHERE ('#39'c'#39' <> '#39'c'#39' ) AND ('
          
            '      ( SAFACT.FechaE BETWEEN CONVERT(DATETIME,'#39'2008-04-01'#39',120)' +
            ' AND CONVERT(DATETIME,'#39'2008-04-30'#39',120) )'
          '       AND ( SAFACT.TipoFac IN ('#39'G'#39','#39'Z'#39') )'
          '       AND ( SAFACT.CodOper = '#39'01-301'#39' ))')
